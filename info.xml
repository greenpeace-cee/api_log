<?xml version="1.0"?>
<extension key="api_log" type="module">
  <file>api_log</file>
  <name>api_log</name>
  <description>Log API requests based on configurable filters</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Patrick Figel</author>
    <email>pfigel@greenpeace.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/greenpeace-cee/api_log</url>
    <url desc="Documentation">https://github.com/greenpeace-cee/api_log/README.md</url>
    <url desc="Support">https://github.com/greenpeace-cee/api_log/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-12-13</releaseDate>
  <version>1.0-alpha.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.63</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/ApiLog</namespace>
    <format>23.02.1</format>
    <angularModule>crmApiLog</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
    <mixin>scan-classes@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_ApiLog_Upgrader</upgrader>
</extension>
